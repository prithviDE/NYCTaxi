{
	"name": "Dataflow_Cleanse_YellowTaxidata",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "clean_yellowtaxi_data",
						"type": "DatasetReference"
					},
					"name": "cleanyellowtaxidata"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "cleaned_yellowdata_to_parquet",
						"type": "DatasetReference"
					},
					"name": "CleanedYellowTaxidata"
				}
			],
			"transformations": [
				{
					"name": "passengercountyellowtaxidata"
				},
				{
					"name": "filterVendor"
				},
				{
					"name": "filterAmountTip"
				},
				{
					"name": "filterPickupDrop"
				}
			],
			"scriptLines": [
				"source(output(",
				"          VendorID as short,",
				"          tpep_pickup_datetime as string,",
				"          tpep_dropoff_datetime as string,",
				"          passenger_count as short,",
				"          trip_distance as double,",
				"          RatecodeID as short,",
				"          store_and_fwd_flag as boolean,",
				"          PULocationID as short,",
				"          DOLocationID as short,",
				"          payment_type as short,",
				"          fare_amount as double,",
				"          extra as double,",
				"          mta_tax as double,",
				"          tip_amount as double,",
				"          tolls_amount as double,",
				"          improvement_surcharge as double,",
				"          total_amount as double,",
				"          congestion_surcharge as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> cleanyellowtaxidata",
				"cleanyellowtaxidata filter(and(passenger_count>=1,passenger_count<=5)) ~> passengercountyellowtaxidata",
				"passengercountyellowtaxidata filter(!(isNull(VendorID)||isNull(payment_type)||isNull(RatecodeID))) ~> filterVendor",
				"filterVendor filter(and(total_amount>0,tip_amount>=0)) ~> filterAmountTip",
				"filterAmountTip filter(and(tpep_pickup_datetime>='2019-11-01',tpep_pickup_datetime<='2019-11-30')\r",
				"&& \r",
				"and(tpep_dropoff_datetime>='2019-11-01',tpep_dropoff_datetime<='2019-11-30')) ~> filterPickupDrop",
				"filterPickupDrop sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0222,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'parquet') ~> CleanedYellowTaxidata"
			]
		}
	}
}